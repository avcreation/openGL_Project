//////////////////////////////////////////////////////
      //////////////////////////////////////////////////////
      //////////////////////////////////////////////////////
      //////////////////////////////////////////////////////
      //////////////////////////////////////////////////////
      // Render skybox shading
      glBindFramebuffer(GL_FRAMEBUFFER, skyboxBuffer.fbo);
      //glClear(GL_COLOR_BUFFER_BIT | GL_DEPTH_BUFFER_BIT);
      glViewport( 0, 0, width, height);
       // bind skybox
       glUseProgram(skybox_shader.program);
      
      // Upload uniforms
      glUniformMatrix4fv(skybox_projectionLocation, 1, 0, projection);
      glUniformMatrix4fv(skybox_viewLocation, 1, 0, worldToView);
      glUniformMatrix4fv(skybox_objectLocation, 1, 0, objectToWorld);
      glUniform1f(skybox_boxLocation, 3);

      // Bind textures
       glActiveTexture(GL_TEXTURE3);
       glBindTexture(GL_TEXTURE_2D, textures[3]);

       glBindVertexArray(vao[7]);
       glDrawElements(GL_TRIANGLES, skybox_triangleCount * 3, GL_UNSIGNED_INT, (void*)0);
       //////////////////////////////////////////////////////
       //////////////////////////////////////////////////////
       //////////////////////////////////////////////////////
       //////////////////////////////////////////////////////
       //////////////////////////////////////////////////////
       //////////////////////////////////////////////////////
       //////////////////////////////////////////////////////
//////////////////////////////////////////////////////



